A,CAAJ;AAAA,6BAAD,C;AAEN,4BAAA,MAAM,EAAA,QAAA,CACf;AAAA,qCAES,CAFT,YAAA,CAES,iBAFT,EAES;AAFA,gCAAA,IAAI,EAAE,KAAA,CAAA;AAEN,+BAFT,EAAwB;kDACtB;AAAA,yCAA2C,CAA3C,YAAA,CAA2C,mBAA3C,EAA2C;AAAhC,oCAAA,OAAK,EAAE,QAAA,CAAA;AAAyB,mCAA3C,EAA4B;sDAAE;AAAA,6CAAE,C,UAAA,CAAF;AAAA,qC,CAAF;;;;AAAA,mCAA5B,E;;AAAA,oC,WAAA,CAA2C,CAA3C;AAAA,iC,CADsB;;;;AAAA,+BAAxB,E;;AAAA,gC,QAAA,CAES,CAFT;AAAA,6BADe,C;8CADjB;AAAA,qCAA0D,CAA1D,YAAA,CAA0D,mBAA1D,EAA0D;AAAxC,gCAAA,KAAK,EAAE,KAAA,CAAA,UAAiC;;yCAAjC,KAAA,CAAA,UAAA,GAAU,M;kCAAuB;AAApB,gCAAA,OAAO,EAAE,QAAA,CAAA;AAAW,+BAA1D,E,IAAA,E;;AAAA,gC,oBAAA,CAA0D,CAA1D;AAAA,6B;;;;2BAFF,E;;AAAA,4B,QAAA,CAZsC,CAAtC;AAAA,yB,CAqBQ;;;;AAAA,uBAtBV,CAsBU,CAtBV;AAAA,qB,CAuBK;;;;AAAA,mBAxBP,CADO,CAFP;AAAA,iB;;;;eADF,CA6BS,EACT,YAAA,CAME,uBANF,EAME;AALC,2BAAO,gBAAG,CAAH;AAAA,yBAAS,CAAC,CAAC,EAAX;AAAA,iBAKR;AAJC,gBAAA,OAAO,EAAE,KAAA,CAAA,OAIV;AAHC,gBAAA,IAAI,EAAE,KAAA,CAAA,IAGP;AAFC,gBAAA,UAAU,EAAE,KAAA,CAAA,UAEb;AADC,gBAAA,OAAO,EAAE,KAAA,CAAA;AACV,eANF,E,IAAA,E;;AAAA,gB,uDAAA,CADS,CA7BT;AAAA,a,CADe;;;;AAAA,WAAjB,CAsCU,CAtCV;AAAA,S;;;;OADF,CAwCD,CAxCC;AAAA,K,CADe;;;;AAAA,GAAjB,C","sourcesContent":["<template>\r\n  <n-space vertical>\r\n    <n-card title=\"班级排课管理\" hoverable>\r\n      <n-space vertical>\r\n        <n-grid x-gap=\"12\" :cols=\"3\">\r\n          <n-gi>\r\n            <class-select v-on:change=\"selectClass = $event\" />\r\n          </n-gi>\r\n          <n-gi>\r\n            <n-space>\r\n              <n-button @click=\"query\">查询</n-button>\r\n              <n-button @click=\"showModal = true\">添加课程</n-button>\r\n              <n-button @click=\"showModalDel = true\">删除课程</n-button>\r\n              <n-modal v-model:show=\"showModal\" preset=\"dialog\">\r\n                <template #header>添加课程</template>\r\n                <course-select v-on:change=\"selAddItem = $event\" />\r\n                <template #action>\r\n                  <n-spin :show=\"inAddItem\">\r\n                    <n-button @click=\"addItem\">添加</n-button>\r\n                  </n-spin>\r\n                </template>\r\n              </n-modal>\r\n              <n-modal v-model:show=\"showModalDel\" preset=\"dialog\">\r\n                <template #header>删除课程</template>\r\n                <n-select v-model:value=\"selDelItem\" :options=\"itemOpt\" />\r\n                <template #action>\r\n                  <n-spin :show=\"inDelItem\">\r\n                    <n-button @click=\"removeItem\">删除</n-button>\r\n                  </n-spin>\r\n                </template>\r\n              </n-modal>\r\n            </n-space>\r\n          </n-gi>\r\n        </n-grid>\r\n        <n-data-table\r\n          :row-key=\"(c) => c.id\"\r\n          :columns=\"columns\"\r\n          :data=\"data\"\r\n          :pagination=\"pagination\"\r\n          :loading=\"inloading\"\r\n        />\r\n      </n-space> </n-card\r\n  ></n-space>\r\n</template>\r\n\r\n<script>\r\nimport { h } from \"vue\";\r\nimport CourseSelect from \"../components/CourseSelect.vue\";\r\nimport {\r\n  NDataTable,\r\n  NText,\r\n  NSpace,\r\n  NButton,\r\n  NGrid,\r\n  NModal,\r\n  NGi,\r\n  NCard,\r\n  NSelect,\r\n  NSpin,\r\n  useMessage,\r\n} from \"naive-ui\";\r\nimport ClassSelect from \"../components/ClassSelect.vue\";\r\n\r\nasync function getItem(cl) {\r\n  let res = await fetch(`https://localhost:5001/api/class:${cl}/course:all`);\r\n  let j = await res.json();\r\n  return j;\r\n}\r\n\r\nexport default {\r\n  components: {\r\n    NDataTable,\r\n    ClassSelect,\r\n    NSpace,\r\n    NButton,\r\n    NGrid,\r\n    NGi,\r\n    NCard,\r\n    CourseSelect,\r\n    NModal,\r\n    NSelect,\r\n    NSpin,\r\n  },\r\n  data() {\r\n    return {\r\n      selectClass: null,\r\n      inloading: false,\r\n      columns: [\r\n        {\r\n          title: \"课程编号\",\r\n          key: \"id\",\r\n        },\r\n        {\r\n          title: \"课程名\",\r\n          key: \"name\",\r\n        },\r\n        {\r\n          title: \"任课教师\",\r\n          key: \"teacherName\",\r\n          render(row) {\r\n            const tags = row.teacherName.map((tagKey) => {\r\n              return h(\r\n                NText,\r\n                {\r\n                  style: {\r\n                    marginRight: \"6px\",\r\n                  },\r\n                },\r\n                {\r\n                  default: () => tagKey,\r\n                }\r\n              );\r\n            });\r\n            return tags;\r\n          },\r\n        },\r\n        {\r\n          title: \"开设学期\",\r\n          key: \"semester\",\r\n        },\r\n        {\r\n          title: \"学时\",\r\n          key: \"hours\",\r\n        },\r\n        {\r\n          title: \"学分\",\r\n          key: \"credit\",\r\n        },\r\n        {\r\n          title: \"考试或考查\",\r\n          key: \"examMethod\",\r\n        },\r\n      ],\r\n      data: [],\r\n      pagination: {\r\n        pageSize: 10,\r\n      },\r\n      showModal: false,\r\n      showModalDel: false,\r\n      inAddItem: false,\r\n      inDelItem: false,\r\n      selAddItem: \"\",\r\n      selDelItem: \"\",\r\n    };\r\n  },\r\n  setup() {\r\n    const msg = useMessage();\r\n    return {\r\n      warn(m) {\r\n        msg.warning(m);\r\n      },\r\n      error(m) {\r\n        msg.error(m);\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    itemOpt() {\r\n      let o = [];\r\n      this.data.forEach((d) => {\r\n        o.push({\r\n          label: d.name,\r\n          value: d.id,\r\n        });\r\n      });\r\n      return o;\r\n    },\r\n  },\r\n  methods: {\r\n    query() {\r\n      if (this.selectClass === null) {\r\n        this.warn(\"请先选择班级\");\r\n      } else {\r\n        this.inloading = true;\r\n        getItem(this.selectClass)\r\n          .then((d) => {\r\n            this.data = d;\r\n            this.inloading = false;\r\n          })\r\n          .catch(() => {\r\n            this.error(\"获取数据失败\");\r\n            this.inloading = false;\r\n          });\r\n      }\r\n    },\r\n    addItem() {\r\n      this.inAddClass = true;\r\n      fetch(\r\n        `https://localhost:5001/api/class:${this.selectClass}/course:${this.selAddItem}`,\r\n        { method: \"POST\" }\r\n      )\r\n        .then(() => {\r\n          this.showModal = false;\r\n          this.inAddItem = false;\r\n          this.inloading = true;\r\n          this.selAddItem = \"\";\r\n          getItem(this.selectClass)\r\n            .then((d) => {\r\n              this.data = d;\r\n              this.inloading = false;\r\n            })\r\n            .catch(() => {\r\n              this.inloading = false;\r\n              this.error(\"数据加载失败\");\r\n            });\r\n        })\r\n        .catch(() => {\r\n          this.error(\"添加失败\");\r\n        });\r\n    },\r\n    removeItem() {\r\n      this.inDelItem = true;\r\n      fetch(\r\n        `https://localhost:5001/api/class:${this.selectClass}/course:${this.selDelItem}`,\r\n        {\r\n          method: \"DELETE\",\r\n        }\r\n      )\r\n        .then(() => {\r\n          this.inDelItem = false;\r\n          this.selDelItem = \"\";\r\n          this.inloading = true;\r\n          this.showModalDel = false;\r\n          getItem(this.selectClass)\r\n            .then((d) => {\r\n              this.data = d;\r\n              this.inloading = false;\r\n            })\r\n            .catch(() => {\r\n              this.inloading = false;\r\n              this.error(\"数据加载失败\");\r\n            });\r\n        })\r\n        .catch(() => {\r\n          this.error(\"删除失败\");\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}