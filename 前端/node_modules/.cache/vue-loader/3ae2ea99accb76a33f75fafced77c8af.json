ei'n/Desktop/mycode/.net/前端/src/components/TeacherSelect.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <n-select clearable v-model:value=\"tvalue\" :multiple=\"multi\" :options=\"options\" :loading=\"inloading\" placeholder=\"请选择教师\"/>\r\n</template>\r\n\r\n<script>\r\nimport { NSelect } from \"naive-ui\";\r\n\r\nasync function getTeachers() {\r\n  let res = await fetch(\"https://localhost:5001/api/teacher:all\");\r\n  let j = await res.json();\r\n  let opt = [];\r\n  j.forEach((t) => {\r\n    opt.push({\r\n      label: t.name,\r\n      value: t.teacherId,\r\n    });\r\n  });\r\n  return opt;\r\n}\r\n\r\nexport default {\r\n  components: {\r\n    NSelect,\r\n  },\r\n  data() {\r\n    return {\r\n      inloading: true,\r\n      tvalue: this.value,\r\n      options: [],\r\n    };\r\n  },\r\n  async mounted() {\r\n    this.options = await getTeachers();\r\n    this.inloading = false;\r\n  },\r\n  props: [\"on-change\", \"value\", \"multi\"],\r\n  watch: {\r\n    tvalue(newVal) {\r\n      this.$emit(\"change\", newVal);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}