;\r\n  j.forEach((t) => {\r\n    opt.push({\r\n      label: t.name,\r\n      value: t.teacherId,\r\n    });\r\n  });\r\n  return opt;\r\n}\r\n\r\nexport default {\r\n  components: {\r\n    NSelect,\r\n  },\r\n  data() {\r\n    return {\r\n      inloading: true,\r\n      tvalue: this.value,\r\n      options: [],\r\n    };\r\n  },\r\n  async mounted() {\r\n    this.options = await getTeachers();\r\n    this.inloading = false;\r\n  },\r\n  props: [\"on-change\", \"value\", \"multi\"],\r\n  watch: {\r\n    tvalue(newVal) {\r\n      this.$emit(\"change\", newVal);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}