   pageSize: 10,\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    showData() {\r\n      if (this.selectClass == null || this.selectClass.length == 0) {\r\n        return this.data;\r\n      }\r\n      return this.data.filter((d) => ~this.selectClass.indexOf(d.classId));\r\n    },\r\n  },\r\n  setup() {\r\n    const msg = useMessage();\r\n    return {\r\n      info(m) {\r\n        msg.info(m);\r\n      },\r\n      warn(m) {\r\n        msg.warning(m);\r\n      },\r\n      error(m) {\r\n        msg.error(m);\r\n      },\r\n    };\r\n  },\r\n  async mounted() {\r\n    this.data = await getAverageScore();\r\n    this.inloading = false;\r\n  },\r\n};\r\n</script>"]}]}